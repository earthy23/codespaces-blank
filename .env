# .env is better suited for public variables, ie, variables that should not commited
# For secret variables is better to use DevServerControl tool with set_env_variable: ["KEY", "SECRET"]

# https://www.builder.io/c/docs/using-your-api-key
VITE_PUBLIC_BUILDER_KEY=__BUILDER_PUBLIC_KEY__
PING_MESSAGE="ping pong"

# Multi-domain configuration
# Comma-separated list of allowed domains
ALLOWED_DOMAINS="localhost:3000,localhost:5173,localhost:8080,ueclub.com,www.ueclub.com,play.ueclub.com,api.ueclub.com,admin.ueclub.com"

# Primary domain for redirects and canonical URLs
PRIMARY_DOMAIN="ueclub.com"

# Enable multi-domain support
MULTI_DOMAIN_ENABLED=true

# WebSocket URLs for different domains
WS_PRODUCTION_URL="wss://ueclub.com"
WS_DEVELOPMENT_URL="ws://localhost:3000"
